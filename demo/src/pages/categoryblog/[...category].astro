---
import { CategoryRoute, categoryHelper } from 'astro-category';
import '@styles/global.css';
export const getStaticPaths = categoryHelper({ collection: 'blogs' });

const { category } = Astro.props;
---

<p>Current path: {category.dir}</p>

<CategoryRoute
  category={category}
  root="categoryblog"
  collection="blogs"
  linkClassName="blog-link red round-20"
/>
